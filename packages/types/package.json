{
  "name": "@solid-explorer/types",
  "version": "1.0.0",
  "description": "Package with the types of Solid Studio",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "precompile": "npm run clean:lib",
    "clean:lib": "rimraf lib",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "tslint --fix -c tslint.json -p tsconfig.json",
    "test": "jest --runInBand --verbose",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,ts,js,json}'",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,json}'",
    "test:coverage": "npm run test -- --reporters=default --reporters=jest-junit --coverage",
    "test:coverage:ci": "npm run test -- --reporters=default --reporters=jest-junit --coverage --coverageReporters=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:solid-studio-open/solid.types.git"
  },
  "author": "Labchain",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.5",
    "jest": "^25.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*.{ts,js}",
      "src/**/*.{ts,js}",
      "!types/**",
      "!test/**",
      "!src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*test.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "typescript": "^3.6.3"
  },
  "gitHead": "e1c70b09b5a06f6242dd58caf784b619e952b7e2"
}
